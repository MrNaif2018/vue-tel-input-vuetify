{
  "cSpell.words": [
    "dropdown",
    "gmail",
    "iamstevendao",
    "keydown",
    "keyup",
    "maxlength",
    "mousemove",
    "nbsp",
    "rgba",
    "tabindex",
    "typeof"
  ],
  "javascript.format.enable": false,
  "eslint.alwaysShowStatus": true,
  "eslint.options": {
    "extensions": [
      ".html",
      ".js",
      ".vue",
      ".jsx"
    ]
  },
  "eslint.validate": ["html", "vue", "javascript", "javascriptreact"],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  }
}
